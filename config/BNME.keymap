#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            // ------------------------------------------------------------------------------------------------------------
            // |  ESC  |  1  |  2  |  3   |  4   |  5   |                   |  6   |  7    |  8    |  9   |   0   |   `   |
            // |  TAB  |  Q  |  W  |  E   |  R   |  T   |                   |  Y   |  U    |  I    |  O   |   P   |   -   |
            // |  CTRL |  A  |  S  |  D   |  F   |  G   |                   |  H   |  J    |  K    |  L   |   ;   |   '   |
            // | SHIFT |  Z  |  X  |  C   |  V   |  B   |   "["  |  |  "]"  |  N   |  M    |  ,    |  .   |   /   | SHIFT |
            //                     | ALT  | GUI  | LOWER|  SPACE |  | ENTER | RAISE| BSPC  | GUI   |

            bindings = <
&kp ESC  &kp N1     &kp N2   &kp N3     &kp N4     &kp N5                     &kp N5     &kp N5    &kp N6     &kp N7  &kp N8  &kp N9
&kp N0   &kp GRAVE  &kp TAB  &kp Q      &kp W      &kp E                      &kp R      &kp T     &kp N5     &kp N5  &kp Y   &kp U
&kp I    &kp O      &kp P    &kp MINUS  &kp LCTRL  &kp A                      &kp S      &kp D     &kp F      &kp G   &kp N5  &kp N5
&kp H    &kp J      &kp K    &kp L      &kp SEMI   &kp SQT                    &kp LSHFT  &kp Z     &kp X      &kp C   &kp V   &kp B
                             &kp N5     &kp N5     &kp N    &kp M  &kp COMMA  &kp DOT    &kp FSLH  &kp RSHFT
            >;
        };

        layer_1 {
            bindings = <
&kp Q   &kp A   &kp T   &kp G   &kp D   &kp N0                  &kp N0  &kp H   &kp E   &trans  &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans                  &trans  &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans                  &trans  &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans                  &trans  &trans  &trans  &trans  &trans  &trans
                        &trans  &trans  &trans  &trans  &trans  &trans  &trans  &trans
            >;
        };
    };
};
